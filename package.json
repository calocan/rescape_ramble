{
  "name": "rescape-ramble",
  "version": "1.0.0",
  "description": "Reimagine our passenger rail system",
  "engines": {
    "node": ">=v7.3.0",
    "npm": ">=4.1.2"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "cross-env NODE_PATH='source' node devServer.js",
    "debug": "cross-env NODE_PATH='source' echo 'Nothing? `npm install -g iron-node`' && iron-node source/debug.js",
    "lint": "eslint source",
    "pretest": "npm run lint",
    "xest": "cross-env $NODE_DEBUG_OPTION NODE_PATH='source' babel-node source/test/index.js",
    "test": "cross-env NODE_PATH='source' babel-tape-runner source/test/index.js",
    "watch": "watch 'clear && npm run test -s' source",
    "update": "updtr",
    "deploy": "firebase deploy",
    "grunt": "grunt responsive_images"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-philpl": "^0.5.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-tape-runner": "^2.0.1",
    "cheerio": "^0.22.0",
    "clean-webpack-plugin": "^0.1.10",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "deep-freeze": "0.0.1",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "grunt-responsive-images": "^1.10.1",
    "json-loader": "^0.5.4",
    "normalize-css": "^2.3.1",
    "postcss-css-variables": "^0.6.0",
    "postcss-easy-import": "^2.0.0",
    "postcss-loader": "^1.2.1",
    "postcss-media-variables": "^2.0.0",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.4.1",
    "redbox-react": "^1.3.3",
    "rimraf": "^2.5.4",
    "tape": "^4.6.3",
    "updtr": "^0.2.3",
    "watch": "^1.0.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.9.0"
  },
  "dependencies": {
    "express": "^4.14.0",
    "history": "^4.5.1",
    "immutable": "^3.8.1",
    "postcss-calc": "^5.3.1",
    "postcss-custom-media": "^5.0.1",
    "react": "^15.3.1",
    "react-datetime": "^2.6.0",
    "react-dimensions": "^1.3.0",
    "react-disqus-thread": "^0.4.0",
    "react-document-meta": "^2.0.3",
    "react-dom": "^15.3.1",
    "react-immutable-proptypes": "^2.1.0",
    "react-map-gl": "^2.0.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.2",
    "react-share": "^1.9.3",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.8",
    "redux-logger": "^2.6.1",
    "redux-map-gl": "^0.1.0",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.1.0",
    "tape-catch": "^1.0.6",
    "xml2js": "^0.4.17"
  }
}

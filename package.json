{
  "name": "rescape-ramble",
  "version": "1.0.0",
  "description": "Reimagine our passenger rail system",
  "engines": {
    "node": ">=v8.0.0",
    "npm": ">=5.0.1"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "nodemon --open --watch webpack.config.js --exec \"webpack-dev-server --env development\"",
    "start-webpack": "webpack-dev-server --progress --hot --port 3000",
    "debug": "cross-env NODE_PATH='source' echo 'Nothing? `npm install -g iron-node`' && iron-node source/debug.js",
    "lint": "eslint source --fix",
    "pretest": "npm run lint",
    "test": "cross-env NODE_PATH='source' BABEL_ENV=test jest",
    "test:watch": "cross-env NODE_PATH='source' jest --watch",
    "test:coverage": "cross-env NODE_PATH='source' jest --coverage",
    "watch": "watch 'clear && npm run test -s' source",
    "update": "updtr",
    "deploy": "firebase deploy",
    "grunt": "grunt responsive_images",
    "webpack": "webpack --progress",
    "webpack-profile": "webpack --profile --json > webpack-stats.json"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jestsetup.js"
    ],
    "testPathIgnorePatterns": [
      "__snapshots__"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "mapbox-gl$": "<rootDir>/node_modules/mapbot-gl/dist/mapbox-gl.js",
      "webworkify": "<rootDir>/node_modules/webworkify-webpack",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "bail": true
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.4.1",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "7.2.0",
    "file-loader": "^0.11.1",
    "gl": "^4.0.3",
    "jasmine": "^2.6.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "json-loader": "^0.5.4",
    "nock": "^9.0.13",
    "pretty-immutable": "^1.0.1",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "shebang-loader": "^0.0.1",
    "sinon": "^3.1.0",
    "source-map-support": "^0.4.15",
    "stream-length": "^1.0.2",
    "style-loader": "^0.18.1",
    "watch": "^1.0.2",
    "watchify": "^3.9.0",
    "webpack": "^3.5.2",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "@cycle/time": "^0.8.0",
    "@mapbox/react-geocoder": "^2.5.0",
    "@turf/turf": "^4.3.1",
    "cycle-pouchdb-most-driver": "^0.0.5",
    "data.maybe": "^1.2.2",
    "data.task": "^3.1.1",
    "deck.gl": "^4.0.6",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.1.0",
    "longjohn": "^0.2.12",
    "luma.gl": "^3.0.2",
    "moment": "^2.18.1",
    "moment-duration-format": "^1.3.0",
    "most": "^1.5.1",
    "pouchdb": "^6.2.0",
    "pouchdb-erase": "^1.0.2",
    "pouchdb-find": "^6.2.0",
    "prop-types": "^15.5.10",
    "query-overpass": "^1.5.0",
    "ramda": "^0.24.1",
    "ramda-fantasy": "^0.8.0",
    "ramda-maybe": "^0.18.0",
    "rbush": "^2.0.1",
    "react": "^15.5.4",
    "react-datetime": "^2.8.10",
    "react-dimensions": "^1.3.0",
    "react-disqus-thread": "^0.4.0",
    "react-document-meta": "^2.1.2",
    "react-dom": "^15.5.4",
    "react-immutable-proptypes": "^2.1.0",
    "react-map-gl": "^3.0.4",
    "react-redux": "^5.0.6",
    "react-router": "4",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-scrollbar": "^0.5.1",
    "redux": "^3.6.0",
    "redux-cycles": "^0.4.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-map-gl": "^0.1.0",
    "redux-pouchdb-plus": "^0.8.1",
    "redux-responsive": "^4.3.2",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.4.0",
    "stack-trace": "^0.0.10"
  }
}

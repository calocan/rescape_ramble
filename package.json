{
  "name": "rescape-ramble",
  "version": "1.0.0",
  "description": "Reimagine our passenger rail system",
  "engines": {
    "node": ">=v7.3.0",
    "npm": ">=4.1.2"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "cross-env NODE_PATH='source' node scripts/devServer.js",
    "start-debug": "cross-env NODE_PATH='source' node $NODE_DEBUG_OPTION scripts/devServer.js",
    "debug": "cross-env NODE_PATH='source' echo 'Nothing? `npm install -g iron-node`' && iron-node source/debug.js",
    "lint": "eslint source",
    "pretest": "npm run lint",
    "test": "cross-env NODE_PATH='source' jest",
    "test:watch": "cross-env NODE_PATH='source' jest --watch",
    "test:coverage": "cross-env NODE_PATH='source' jest --coverage",
    "testX": "cross-env NODE_PATH='source' browserify --debug -x react/addons -x react/lib/ExecutionEnvironment -x react/lib/ReactContext -t babelify ./source/**/**/*.spec.js | tape-run -b chrome -p 2222",
    "watch": "watch 'clear && npm run test -s' source",
    "update": "updtr",
    "deploy": "firebase deploy",
    "grunt": "grunt responsive_images"
  },
  "jest": {
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-philpl": "^0.5.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.3.0",
    "clean-webpack-plugin": "^0.1.10",
    "concurrently": "^3.1.0",

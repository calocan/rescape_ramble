{
  "name": "rescape-ramble",
  "version": "1.0.0",
  "description": "Reimagine our passenger rail system",
  "engines": {
    "node": ">=v8.0.0",
    "npm": ">=5.0.1"
  },
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --hot",
    "dev-server": "nodemon ./scripts/devServer.js localhost 3100",
    "dev-client": "webpack-dev-server --port 3000",
    "clean": "rimraf build",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "debug": "cross-env NODE_PATH='src' echo 'Nothing? `npm install -g iron-node`' && iron-node src/debug.js",
    "lint": "eslint src --fix",
    "pretest": "npm run lint",
    "test": "cross-env NODE_PATH='src' BABEL_ENV=test jest",
    "test:watch": "cross-env NODE_PATH='src' jest --watch",
    "test:coverage": "cross-env NODE_PATH='src' jest --coverage",
    "watch": "watch 'clear && npm run test -s' src",
    "update": "updtr",
    "deploy": "firebase deploy",
    "webpack": "webpack --progress",
    "webpack-profile": "webpack --profile --json > webpack-stats.json"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/source"
    ],
    "setupFiles": [
      "<rootDir>/jestsetup.js"
    ],
    "testPathIgnorePatterns": [
      "__snapshots__"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "mapbox-gl$": "<rootDir>/node_modules/mapbot-gl/dist/mapbox-gl.js",
      "webworkify": "<rootDir>/node_modules/webworkify-webpack",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "bail": true
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.1.1",
    "eslint": "^4.8.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "gl": "^4.0.4",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "json-loader": "^0.5.7",
    "nock": "^9.0.21",
    "nodemon": "^1.12.1",
    "pretty-immutable": "^1.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.2",
    "shebang-loader": "^0.0.1",
    "sinon": "^4.0.1",
    "source-map-support": "^0.5.0",
    "stream-length": "^1.0.2",
    "style-loader": "^0.19.0",
    "watch": "^1.0.2",
    "watchify": "^3.9.0",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "@cycle/time": "^0.10.0",
    "@mapbox/react-geocoder": "^2.5.0",
    "@turf/turf": "^4.7.3",
    "d3": "^4.11.0",
    "d3-sankey": "^0.7.1",
    "data.maybe": "^1.2.2",
    "data.task": "^3.1.1",
    "deck.gl": "^4.1.3",
    "enzyme": "^3.1.0",
    "express": "^4.16.1",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.3.0",
    "longjohn": "^0.2.12",
    "luma.gl": "^4.0.4",
    "moment": "^2.18.1",
    "moment-duration-format": "^1.3.0",
    "pouchdb": "^6.3.4",
    "pouchdb-erase": "^1.0.2",
    "prop-types": "^15.6.0",
    "query-overpass": "^1.5.1",
    "ramda": "^0.24.1",
    "ramda-fantasy": "^0.8.0",
    "ramda-lens": "^0.1.2",
    "ramda-maybe": "^0.18.0",
    "rbush": "^2.0.1",
    "react": "^16.0.0",
    "react-datetime": "^2.10.3",
    "react-dimensions": "^1.3.0",
    "react-disqus-thread": "^0.4.0",
    "react-document-meta": "^2.1.2",
    "react-dom": "^16.0.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-map-gl": "^3.0.5",
    "react-redux": "^5.0.6",
    "react-router": "4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scrollbar": "^0.5.1",
    "redux": "^3.7.2",
    "redux-cycles": "^0.4.1",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-map-gl": "^0.1.0",
    "redux-responsive": "^4.3.3",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.2.0",
    "reflexbox": "^3.0.0-0",
    "rescape-cycle": "^1.5.7",
    "rescape-ramda": "^1.2.6",
    "rescape-validate": "^1.2.3",
    "reselect": "^3.0.1",
    "stack-trace": "^0.0.10"
  }
}
